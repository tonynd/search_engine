<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">


 <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>

	<servlet>
    	<servlet-name>initialization</servlet-name>
    	<servlet-class>search.Initialization</servlet-class>
  	</servlet>
  	<servlet-mapping>
    	<servlet-name>initialization</servlet-name>
    	<url-pattern>/initIndex</url-pattern>
  	</servlet-mapping>
  	
	<servlet>
    	<servlet-name>results</servlet-name>
    	<servlet-class>search.PrintResult</servlet-class>
  	</servlet>
  	<servlet-mapping>
    	<servlet-name>results</servlet-name>
    	<url-pattern>/printResult</url-pattern>
  	</servlet-mapping>
</web-app>